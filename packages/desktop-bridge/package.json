{
  "name": "@resonance/desktop-bridge",
  "version": "0.1.0",
  "description": "TypeScript IPC types and utilities for Tauri desktop integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./discord": {
      "types": "./dist/discord.d.ts",
      "import": "./dist/discord.js"
    },
    "./media-keys": {
      "types": "./dist/media-keys.d.ts",
      "import": "./dist/media-keys.js"
    },
    "./tray": {
      "types": "./dist/tray.d.ts",
      "import": "./dist/tray.js"
    },
    "./window": {
      "types": "./dist/window.d.ts",
      "import": "./dist/window.js"
    },
    "./environment": {
      "types": "./dist/environment.d.ts",
      "import": "./dist/environment.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@tauri-apps/api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
